{
  "name": "blackcater",
  "displayName": "Krawl",
  "version": "0.0.1",
  "private": true,
  "description": "Support krawl crawler framework",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blackcater-labs/krawl.git",
    "directory": "packages/ext-vscode"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/extension.js",
  "engines": {
    "vscode": "^1.85.0"
  },
  "activationEvents": [],
  "contributes": {
    "commands": [
      {
        "command": "blackcater.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "dev": "esno ./scripts/dev.ts && unbuild --stub",
    "build": "unbuild",
    "publish": "esno ./scripts/publish.ts",
    "pack": "vsce package --no-dependencies"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.10.4",
    "@types/vscode": "^1.85.0",
    "esno": "^4.0.0",
    "execa": "^8.0.1",
    "fs-extra": "^11.2.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0"
  }
}
